{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "bracketSpacing": true,
  "vueIndentScriptAndStyle": false,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
  "quoteProps": "as-needed",
  "arrowParens": "avoid",

  "plugins": ["prettier-plugin-organize-imports"]
}
